{
  "functions": {
    "source": "functions",
    "destination": "/index.js"
  },
  "hosting": {
    "public": "Pagina/dist/formReactivo",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html",
        "function": "formulario"
      }
    ]
  }
}
